{
  "presets": ["babel-preset-expo"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@app": "./src",
          "@assets": "./assets",
          "@components": "./src/components",
          "@pages": "./src/Pages",
          "@services": "./src/services",
          "@utils": "./src/utils"
        }
      }
    ]
  ]
}